		<param type="label" text="Type" x="10" y="10" w="120" h="13"/>
		<param type="select" id="input_type" text="" def="text" x="130" y="8" w="180" h="13">
			<items>
				<item id="text" label="Text Input">
					<events>
						<event name="selendok" target="lab_input_maxlength" action="show"/>
						<event name="selendok" target="input_maxlength" action="show"/>
						<event name="selendok" target="lab_input_maxlength2" action="show"/>
						<event name="selendok" target="lab_input_wrap" action="hide"/>
						<event name="selendok" target="input_wrap" action="hide"/>
					</events>
				</item>
				<item id="password" label="Hidden Input">
					<events>
						<event name="selendok" target="lab_input_maxlength" action="show"/>
						<event name="selendok" target="input_maxlength" action="show"/>
						<event name="selendok" target="lab_input_maxlength2" action="show"/>
						<event name="selendok" target="lab_input_wrap" action="hide"/>
						<event name="selendok" target="input_wrap" action="hide"/>
					</events>
				</item>
				<item id="textarea" label="Text Area">
					<events>
						<event name="selendok" target="lab_input_maxlength" action="hide"/>
						<event name="selendok" target="input_maxlength" action="hide"/>
						<event name="selendok" target="lab_input_maxlength2" action="hide"/>
						<event name="selendok" target="lab_input_wrap" action="show"/>
