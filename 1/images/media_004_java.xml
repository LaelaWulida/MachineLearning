--><params>
	<object>
		<label>Java-applet</label>
		<width>240</width>
		<height>240</height>
		<description>Java-applet</description>
		<state_extension>false</state_extension>
	</object>
    <page name="Main" w="260" h="150">
		<param type="edit" id="hidden.label.applet" text="" def="Applet" x="10" y="-50" w="1" h="1"/>
	
		<param type="check" id="use_jar" text="JAR-archive" def="no" x="10" y="8" w="120" h="13">
			<events>
				<event name="check" target="applet_uri" action="enable"/>
				<event name="check" target="label_stclass" action="enable"/>
				<event name="check" target="class_start" action="enable"/>
				<event name="check" target="label_class" action="disable"/>
				<event name="check" target="class_uri" action="disable"/>
				<event name="check" target="lab_file_list" action="disable"/>
				<event name="check" target="file_list" action="disable"/>
				<event name="uncheck" target="applet_uri" action="disable"/>
				<event name="uncheck" target="lab_file_list" action="enable"/>
				<event name="uncheck" target="file_list" action="enable"/>
				<event name="uncheck" target="label_stclass" action="disable"/>
				<event name="uncheck" target="class_start" action="disable"/>
				<event name="uncheck" target="label_class" action="enable"/>
				<event name="uncheck" target="class_uri" action="enable"/>
			</events>
		</param>
		<param type="file" id="applet_uri" text="" def="" x="130" y="8" w="103" h="13" ext="JAR-archives|*.jar|All files|*.*"/>
		<param type="label" id="label_stclass" text="Start file" def="no" x="10" y="26" w="120" h="13"/>
		<param type="edit" id="class_start" text="" def="" x="130" y="24" w="120" h="13"/>

		<param type="label" id="label_class" text="Class file" def="no" x="10" y="42" w="120" h="13"/>
		<param type="file" id="class_uri" text="" def="" x="130" y="40" w="103" h="13"/>

		<param type="label" id="lab_file_list" text="Additional files" x="10" y="58" w="120" h="13"/>
		<param type="list" id="file_list" text="" def="" x="10" y="56" w="240" h="74">
			<params w="260" h="50" label="Additional file">
				<param type="label" text="Additional file" x="10" y="10" w="120" h="13"/>
				<param type="file" id="file_uri" text="File" def="" x="130" y="8" w="103" h="13"/>
				<param type="line" x="10" y="26" w="240" h="1"/>
				<param type="button" id="OK" text="OK" x="145" y="34" w="50" h="14"/>
				<param type="button" id="CANCEL" text="Cancel" x="200" y="34" w="50" h="14"/>
			</params>
		</param>
    </page>
    <page name="Parameters" w="260" h="150">
		<param type="label" text="Horizontal margin" x="10" y="10" w="120" h="13"/>
		<param type="spinner" id="applet_hspace" text="" def="0" x="110" y="8" w="32" h="13" min="0" max="99"/>
		<param type="label" text="px" x="165" y="10" w="30" h="13"/>
		<param type="label" text="Vertical margin" x="10" y="26" w="120" h="13"/>
		<param type="spinner" id="applet_vspace" text="" def="0" x="110" y="24" w="32" h="13" min="0" max="99"/>
		<param type="label" text="px" x="165" y="26" w="30" h="13"/>
		<param type="label" text="Alternative text" x="10" y="42" w="120" h="13"/>
		<param type="edit" id="applet_alt" text="" def="Java-applet" x="130" y="40" w="120" h="13"/>
		<param type="label" id="lab_file_list" text="Parameters" x="10" y="58" w="120" h="13"/>
		<param type="list" id="param_list" text="" def="" x="10" y="56" w="240" h="86">
			<params w="260" h="70" label="Parameter">
				<param type="label" text="Parameter" x="10" y="10" w="120" h="13"/>
				<param type="edit" id="a_param" text="Parameter" def="" x="130" y="8" w="120" h="13"/>
				<param type="label" text="Value" x="10" y="26" w="120" h="13"/>
				<param type="edit" id="a_param_value" text="Value" def="" x="130" y="24" w="120" h="13"/>
				<param type="line" x="10" y="42" w="240" h="1"/>
				<param type="button" id="OK" text="OK" x="145" y="50" w="50" h="14"/>
				<param type="button" id="CANCEL" text="Cancel" x="200" y="50" w="50" h="14"/>
			</params>
		</param>
    </page>
</params>